  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.coupon .couponList .bottomBlock .right .rightIcon {
  margin-top: 3px;
}
.coupon .expired {
  border-top: 3px solid #9b9b9b;
}
.coupon .expired .leftBlock {
  color: #9b9b9b !important;
  -webkit-box-align: center !important;
  -webkit-align-items: center !important;
          align-items: center !important;
}
.coupon .expired .leftBlock .center {
  margin-top: 0px;
  margin-left: 20px !important;
}
.coupon .expired .leftBlock .center .top {
  color: #9b9b9b !important;
  margin-top: 15px;
}
.coupon .expired .leftBlock .center .time {
  margin-top: 10px;
}
.coupon .expired .topBlock {
  border-bottom: 0;
}
.coupon .expired .topBlock ::before {
  width: 0px;
  height: 0px;
}
.coupon .expired .topBlock ::after {
  width: 0px;
  height: 0px;
}
.coupon .noCoupon {
  margin: auto;
  margin-top: 200rpx;
  width: 300rpx;
}
.coupon .noCoupon .title {
  font-size: 17px;
  text-align: center;
}
