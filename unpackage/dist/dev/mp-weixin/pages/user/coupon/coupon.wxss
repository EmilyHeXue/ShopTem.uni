.coupon {
  background-color: #f8f8f8;
  height: 100vh;
}
.coupon .navBar {
  background-color: #fff;
  padding: 15px 15px 0 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #676767;
  font-weight: bold;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.coupon .navBar .navBarlist {
  padding-bottom: 15px;
}
.coupon .couponList {
  margin: 10px 15px 0 15px;
  background-color: #fff;
  -webkit-border-radius: 10px;
          border-radius: 10px;
  font-size: 15px;
  padding: 15px 12px 8px 15px;
  -webkit-box-shadow: 0 0 15px #ebebeb;
          box-shadow: 0 0 15px #ebebeb;
  border-top: 3px solid #62b34c;
  position: relative;
  overflow: hidden;
}
.coupon .couponList .topBlock {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px dashed #ebebeb;
  padding-bottom: 20px;
}
.coupon .couponList .topBlock ::before {
  width: 13px;
  height: 13px;
  content: '';
  position: absolute;
  left: -7px;
  bottom: 46px;
  background-color: #ececec;
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.coupon .couponList .topBlock ::after {
  width: 13px;
  height: 13px;
  content: '';
  position: absolute;
  right: -7px;
  bottom: 46px;
  background-color: #ececec;
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.coupon .couponList .topBlock .leftBlock {
  color: #ec8131;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
}
.coupon .couponList .topBlock .leftBlock .price {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: baseline;
  -webkit-align-items: baseline;
          align-items: baseline;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  font-weight: bold;
  margin-top: 5px;
}
.coupon .couponList .topBlock .leftBlock .price .number {
  font-size: 30px;
}
.coupon .couponList .topBlock .leftBlock .condition {
  font-size: 22rpx;
  margin-top: -5px;
}
.coupon .couponList .topBlock .center {
  margin-top: 5px;
  margin-left: 25rpx;
}
.coupon .couponList .topBlock .center .top {
  font-size: 17px;
  color: #000;
  font-weight: bold;
}
.coupon .couponList .topBlock .center .time {
  font-size: 22rpx;
  color: #9b9b9b;
}
.coupon .couponList .topBlock .center .condition {
  color: #9b9b9b;
  margin-top: 0;
}
.coupon .couponList .topBlock .rightBlock .remind {
  color: #ec8131;
  font-size: 22rpx;
  text-align: center;
}
.coupon .couponList .topBlock .rightBlock .bottomButton {
  text-align: center;
  color: #fff;
  background-color: #0071ce;
