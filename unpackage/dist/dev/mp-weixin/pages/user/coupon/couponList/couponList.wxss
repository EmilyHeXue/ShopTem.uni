.blueActive {
  border-bottom: 2px solid #0071ce;
  color: #0071ce;
}
.couponCenter {
  background-color: #f8f8f8;
  height: 100vh;
}
.couponCenter .navBar {
  background-color: #fff;
  padding: 15px 15px 0 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #676767;
  font-weight: bold;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.couponCenter .navBar .navBarlist {
  padding-bottom: 15px;
}
.couponCenter .couponList {
  margin: 10px 15px 0 15px;
  background-color: #fff;
  -webkit-border-radius: 10px;
          border-radius: 10px;
  font-size: 15px;
  padding: 15px 12px 8px 15px;
  -webkit-box-shadow: 0 0 15px #ebebeb;
          box-shadow: 0 0 15px #ebebeb;
  border-top: 3px solid #62b34c;
  position: relative;
  overflow: hidden;
}
.couponCenter .couponList .topBlock {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding-bottom: 20px;
}
.couponCenter .couponList .topBlock .leftBlock {
  color: #ec8131;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
}
.couponCenter .couponList .topBlock .leftBlock .price {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: baseline;
  -webkit-align-items: baseline;
          align-items: baseline;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  font-weight: bold;
  margin-top: 5px;
}
.couponCenter .couponList .topBlock .leftBlock .price .number {
  font-size: 30px;
}
.couponCenter .couponList .topBlock .leftBlock .condition {
  font-size: 22rpx;
  margin-top: -5px;
}
.couponCenter .couponList .topBlock .center {
  margin-top: 30rpx;
  margin-left: 20rpx;
}
.couponCenter .couponList .topBlock .center .top {
  font-size: 34rpx;
  color: #000;
  font-weight: bold;
}
.couponCenter .couponList .topBlock .center .time {
  font-size: 22rpx;
  margin-top: 15rpx;
  color: #9b9b9b;
}
.couponCenter .couponList .topBlock .rightBlock .remind {
  color: #ec8131;
  font-size: 22rpx;
  text-align: center;
}
.couponCenter .couponList .topBlock .rightBlock .bottomButton {
  text-align: center;
  color: #fff;
  background-color: #0071ce;
  padding: 10rpx 15rpx;
  font-size: 28rpx;
  margin-top: 3px;
  -webkit-border-radius: 30px;
          border-radius: 30px;
}
.couponCenter .noCoupon {
  margin: auto;
  margin-top: 200rpx;
  width: 300rpx;
}
.couponCenter .noCoupon .title {
  font-size: 17px;
}

